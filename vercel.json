{
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path((?!\\.well-known/(?:apple-app-site-association|assetlinks\\.json)$).*)",
      "has": [
        { "type": "host", "value": "bookitgy.com" }
      ],
      "destination": "https://www.bookitgy.com/:path*",
      "permanent": true
    }
  ]
}
